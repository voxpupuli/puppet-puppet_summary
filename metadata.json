{
  "name": "puppet-puppet_summary",
  "version": "1.0.0-rc0",
  "summary": "Manages the puppet-summary service (report processor)",
  "author": "Vox Pupuli",
  "dependencies": [
    {
      "name": "camptocamp/systemd",
      "version_requirement": ">= 3.0.0 < 4.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 7.1.0 < 8.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 5.0.0 < 6.0.0"
    }
  ],
  "source": "https://github.com/voxpupuli/puppet-puppet_summary.git",
  "project_page": "https://github.com/voxpupuli/puppet-puppet_summary",
  "issues_url": "https://github.com/voxpupuli/puppet-puppet_summary/issues",
